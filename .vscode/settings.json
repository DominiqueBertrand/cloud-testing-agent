{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript"
    ],
    "sqltools.useNodeRuntime": true,
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "database": "${workspaceFolder:coog-cloud-agent}/coog-cloud-agent.db.sqlite",
            "name": "local/coog-cloud-agent",
            "group": "coog-cloud-agent"
        }
    ],
    "sonarlint.connectedMode.project": {
        "connectionId": "http-localhost-9000",
        "projectKey": "coog-cloud-agent"
    }
}